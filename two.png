			{{#hasClose}}
				<div class="close"><i class="icon icon-close"></i></div>
			{{/hasClose}}
		</div>
		<div class="popup-navigation">
			{{{navigationHtml}}}
		</div>
		<div class="popup-content">
			<div class="content">
				{{{contentHtml}}}
			</div>
		</div>
		<div class="popup-footer">
			{{{footerHtml}}}
		</div>
		
	</div>
</script>

<script id="generic.popUpNavigation" type="text/template">
